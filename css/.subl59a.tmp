html,body,div,span,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,abbr,address,cite,code,del,dfn,em,img,ins,kbd,q,samp,small,strong,sub,sup,var,b,i,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,menu,nav,section,summary,time,mark,audio,video,button {
    margin: 0;
    padding: 0;
    border: 0;
    outline: 0;
    list-style: none;
    font-family: "Open Sans","Helvetica Neue",Helvetica,Arial,sans-serif
}

article,aside,footer,header,hgroup,main,nav,section {
    display: block
}

body {
    min-width: 320px;
    max-width: 640px;
    height: 100%;
    word-wrap: break-word;
    word-break: break-all;
    padding: 0;
    margin: 0 auto;
    color: #676767;
    font: 16px/20px "microsoft yahei","Open Sans","Helvetica Neue",Helvetica,Arial,sans-serif;
    background-color: #fff;
    overflow-x: hidden
}

html {
    -webkit-text-size-adjust: 100%
}

html,body {
    -webkit-touch-callout: none
}

* {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    margin: 0;
    padding: 0;
    -webkit-font-smoothing: antialiased;
    -webkit-tap-highlight-color: rgba(255,255,255,0);
    letter-spacing: -0.03em
}

a {
    color: #676767;
    text-decoration: none
}

a:hover {
    text-decoration: none
}

img {
    border: 0;
    vertical-align: top
}

table {
    border-collapse: collapse;
    border-spacing: 0
}

th,td,caption {
    vertical-align: top;
    text-align: left
}

sub,sup,samp {
    font-size: 10px;
    font-style: normal
}

.clearfix:before,.clearfix:after {
    content: "";
    display: table
}

.clearfix:after {
    clear: both
}

.cl {
    clear: both
}

.ta-c {
    text-align: center
}

.ta-l {
    text-align: left
}

.ta-r {
    text-align: right
}

.fl {
    float: left
}

.fr {
    float: right
}

#iSlider {
    position: absolute;
    height: 100%;
    width: 100%;
    overflow: hidden;
    z-index: 0;
}

.content {
  padding: 10%;
  height: 100%;
  width: 100%;
  -webkit-box-sizing: border-box;
}

.content h1{
  margin: 2em 0;
}

.content h2{
  margin: 2em 0;  
}

.content p{
  margin: 1em 0;  
}

.loading-page {
  position: absolute;
  top: 0;
  left: 0;
  background-color: #3fa8f8;
  color: #FFF;
  width: 100%;
  height: 100%;
  background-size: 300px auto;
}

.page1{
  background-color: #3fa8f8;
  color: #FFF;
}
.page2{
  background-color: #90FFB7;  
}
.page3{
  background-color: #90ABFF;  
  color: #FFF;
}
.page4{
  background-color: #FCFF90;  
}
.page5{
  background-color: #E390FF;
  color: #FFF;  
}
.page4{
  background-color: #FCFF90;  
}
.page5{
  background-color: #E390FF;
  color: #FFF;  
}
/* person */
.loading-page .persons {
    height: 220px;
    margin-top: 60px;
    width: 100%;
    position: relative;
    overflow: hidden
}

.persons .p01,.persons .p02,.persons .p03,.persons .p04,.persons .p05,.persons .p06,.persons .p07,.persons .p08,.persons .p09,.persons .p10 {
    position: absolute;
    display: block;
    background: url("../images/fkeybord.png") no-repeat;
    background-size: 75px 600px
}

.persons .p01 {
    width: 66px;
    height: 66px;
    background-position: 0 -114px;
    right: 65%;
    top: 32px
}

.persons .p02 {
    width: 36px;
    height: 36px;
    background-position: 0 -69px;
    top: 35px;
    right: 50%
}

.persons .p03 {
    width: 62px;
    height: 62px;
    background-position: 0 0;
    top: 20px;
    left: 68%
}

.persons .p04 {
    width: 46px;
    height: 46px;
    background-position: 0 -186px;
    top: 60px;
    left: 55%
}

.persons .p05 {
    width: 56px;
    height: 56px;
    background-position: 0 -540px;
    top: 80px;
    left: 73%
}

.persons .p06 {
    width: 56px;
    height: 56px;
    background-position: 0 -241px;
    top: 86px;
    right: 50%;
    margin-right: -20px
}

.persons .p07 {
    width: 42px;
    height: 42px;
    background-position: 0 -366px;
    top: 124px;
    right: 66%
}

.persons .p08 {
    width: 36px;
    height: 36px;
    background-position: 0 -310px;
    top: 118px;
    left: 58%
}

.persons .p09 {
    width: 58px;
    height: 58px;
    background-position: 0 -420px;
    top: 152px;
    right: 50%;
    margin-right: -15px
}

.persons .p10 {
    width: 44px;
    height: 44px;
    background-position: 0 -486px;
    top: 158px;
    left: 50%;
    margin-left: 27px
}
.persons div {
    position: absolute;
    color: #fff;
    -webkit-transition: All .5s ease;
    /*opacity: 0*/
}

.persons div.t1 {
    font-size: 18px;
    top: 5px;
    right: 50%;
    margin-right: -58px;
    -webkit-transform: translate(25px,0)
}

.persons div.t2 {
    font-size: 13px;
    left: 71%;
    top: 140px;
    -webkit-transform: translate(20px,0)
}

.persons div.t3 {
    font-size: 12px;
    top: 98px;
    right: 62%;
    -webkit-transform: translate(-20px,0)
}

.persons div.t4 {
    font-size: 12px;
    top: 162px;
    right: 64%;
    -webkit-transform: translate(-20px,0)
}
.load-panel {
    position: absolute;
    width: 100%;
    height: 80px;
    left: 0;
    bottom: 50px;
    padding: 0 70px
}

.process-box {
    height: 3px;
    position: relative;
    margin-top: 12px
}

.process-box .process {
    height: 1px;
    width: 100%;
    background-color: #a3daea;
    overflow: hidden;
    position: absolute;
    top: 1px;
    left: 0
}

.process-box .process-percent {
    height: 3px;
    background-color: #ffffff;
    position: absolute;
    left: 0;
    top: 0;
    width: 50%
}
.load-panel p {
    color: #fff;
    text-align: center;
    font-size: 14px;
    margin-top: 10px
}